    </div>

    <div class="col-lg-11">

        <fieldset style="height:17px"">

            @Html.DisplayFor(model => model.CreditorName)<span>&nbsp; | &nbsp;</span>

            @Html.DisplayNameFor(model => model.CreditorIdLong)
            @Html.DisplayFor(model => model.CreditorIdLong)<span>&nbsp; | &nbsp;</span>

            @Html.DisplayNameFor(model => model.CreditorIdShort)
            @Html.DisplayFor(model => model.CreditorIdShort)
        </fieldset>
        <hr />
        <fieldset style="height:17px"">
            @Html.DisplayFor(model => model.RegName)<span>&nbsp; | &nbsp;</span>

            @Html.DisplayNameFor(model => model.IdShort)
            @Html.DisplayFor(model => model.IdShort)<span>&nbsp; | &nbsp;</span>

            @Html.DisplayNameFor(model => model.IdLong)
            @Html.DisplayFor(model => model.IdLong)<span>&nbsp; | &nbsp;</span>

            @Html.DisplayNameFor(model => model.Start)
            @Html.DisplayFor(model => model.Start)<span>&nbsp; | &nbsp;</span>

            @Html.DisplayNameFor(model => model.Stop)
            @Html.DisplayFor(model => model.Stop)
        </fieldset>
        <hr />
        <fieldset style="height:17px"" class=@Html.GetAlertType(Model.ArchiveFlag)>
            @Html.DisplayNameFor(model => model.BusinessN)
            @Html.DisplayFor(model => model.BusinessN)<span>&nbsp; | &nbsp;</span>

            @Html.DisplayNameFor(model => model.DebtDogovorN)
            @Html.DisplayFor(model => model.DebtDogovorN)<span>&nbsp; | &nbsp;</span>

            @Html.DisplayNameFor(model => model.ProjectId)
            @Html.DisplayFor(model => model.ProjectId)<span>&nbsp; | &nbsp;</span>
        </fieldset>
        <hr />
        <fieldset style="height:17px"">
            @Html.DisplayNameFor(model => model.DebtRest)
            @Html.DisplayFor(model => model.DebtRest)<span>&nbsp; | &nbsp;</span>

            @Html.DisplayNameFor(model => model.TotalRest)
            @Html.DisplayFor(model => model.TotalRest)
        </fieldset>
        <hr />
        <fieldset style="height:17px"">

            @Html.DisplayFor(model => model.NameF)
            @Html.DisplayFor(model => model.NameI)
            @Html.DisplayFor(model => model.NameO) <span>&nbsp; | &nbsp;</span>

            @Html.DisplayNameFor(model => model.Inn)
            @Html.DisplayFor(model => model.Inn)
        </fieldset>
        <hr />
        <fieldset style="height:17px"">
            @Html.DisplayFor(model => model.Ap)
        </fieldset>
        <hr />
        <fieldset style="height:17px"">
            @Html.DisplayFor(model => model.Af)
        </fieldset>
        <hr />
        <fieldset style="height:17px"">
            @Html.DisplayFor(model => model.Avr)
        </fieldset>
        <hr />
        <fieldset style="height:17px"">            
            @Html.DisplayFor(model => model.Aw)
        </fieldset>
        <hr />
        <fieldset style="height:17px"">            
            @Html.DisplayFor(model => model.Marker1)<span>&nbsp; &nbsp; &nbsp;</span>

            @Html.DisplayFor(model => model.Period1)
        </fieldset>
        <hr />
        <fieldset style="height:17px"">
              @Html.DisplayFor(model => model.Marker2)<span>&nbsp; &nbsp; &nbsp;</span>

            @Html.DisplayFor(model => model.Period2)
        </fieldset>
        <hr />

    </div>












