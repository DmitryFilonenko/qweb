@model WebUI.Models.QEntities.Pin


<div>
    <div class="col-lg-1">
        <p style="height:17px"">@Html.DisplayNameFor(model => model.CreditorName)</p>
        <hr />
        <p style="height:17px"">@Html.DisplayNameFor(model => model.RegName)</p>
        <hr />
        <p style="height:17px"" class=@Html.GetAlertType(Model.ArchiveFlag)>Дело:</p>
        <hr />
        <p style="height:17px"">Долг:</p>
